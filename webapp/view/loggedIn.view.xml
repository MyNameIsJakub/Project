<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:tnt="sap.tnt" controllerName="Project.Decidalo.controller.loggedIn">
	<Shell id="shell">
		<Page title="App Title">
		<headerContent>
		</headerContent>
		<content>
		    <sap.ui.layout:Grid xmlns:sap.ui.layout="sap.ui.layout" id="grid0">
		        <sap.ui.layout:content>
		            <ObjectListItem title="Tool 2" icon="sap-icon://projector" id="tool2"/>
		            <ObjectListItem title="Tool 3" icon="sap-icon://print" id="tool3"/></sap.ui.layout:content>
		    </sap.ui.layout:Grid>
		</content>
		<customHeader>
		    <Bar xmlns="sap.m" id="bar0">
		        <contentRight>
		            <Button xmlns="sap.m" id="userButton" icon="sap-icon://drop-down-list" press="handleOpen" text="{/loginData/user}"/>
		        </contentRight>
		    </Bar>
		</customHeader>
		</Page>
